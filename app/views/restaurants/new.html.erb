<h1>New restaurant</h1>
<%= simple_form_for [@restaurant] do |f|%>
  <%= f.input :name %>
  <%= f.input :category, collection: Restaurant::CATEGORIES%>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.submit %>
<% end %>
<%= link_to 'Back', restaurants_path, class: "btn btn-primary" %>
